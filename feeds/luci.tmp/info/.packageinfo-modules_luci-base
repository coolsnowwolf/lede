     config LUCI_LANG_es
	   tristate "Spanish (es)" 

     config LUCI_LANG_fa
	   tristate "Persian (fa)" 

     config LUCI_LANG_fi
	   tristate "Finnish (fi)" 

     config LUCI_LANG_fil
	   tristate "Philippinic (fil)" 

     config LUCI_LANG_fr
	   tristate "French (fr)"  

     config LUCI_LANG_he
	   tristate "Hebrew (he)" 

     config LUCI_LANG_hi
