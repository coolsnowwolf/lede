     config LUCI_LANG_hu
	   tristate "Hungarian (hu)"  

     config LUCI_LANG_it
	   tristate "Italian (it)" 

     config LUCI_LANG_ja
	   tristate "Japanese (ja)" 

     config LUCI_LANG_ko
	   tristate "Korean (ko)" 

     config LUCI_LANG_lt
	   tristate "Lithuanian (lt)"  

     config LUCI_LANG_mr
	   tristate "Marathi (mr)" 

     config LUCI_LANG_ms
	   tristate "Malay (ms)" 

     config LUCI_LANG_nb_NO
	   tristate "Norwegian (nb_NO)" 

     config LUCI_LANG_nl
	   tristate "Dutch (nl)" 

     config LUCI_LANG_pl
	   tristate "Polish (pl)" 

     config LUCI_LANG_pt
	   tristate "Portuguese (pt)" 

     config LUCI_LANG_pt_BR
	   tristate "Brazilian Portuguese (pt_BR)" 

     config LUCI_LANG_ro
	   tristate "Romanian (ro)" 

     config LUCI_LANG_ru
	   tristate "Russian (ru)" 

     config LUCI_LANG_sk
