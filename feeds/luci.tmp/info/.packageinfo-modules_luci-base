Config:
   config LUCI_SRCDIET
	bool "Minify Lua sources"
	default n

   config LUCI_JSMIN
	bool "Minify JavaScript sources"
	default n

   config LUCI_CSSTIDY
	bool "Minify CSS files"
	default n

   menu "Translations"

     config LUCI_LANG_ar
	   tristate "Arabic (ar)"  

     config LUCI_LANG_bg
	   tristate "Bulgarian (bg)" 

     config LUCI_LANG_bn_BD
	   tristate "Bengali (bn_BD)" 

     config LUCI_LANG_ca
	   tristate "Catalan (ca)" 

     config LUCI_LANG_cs
	   tristate "Czech (cs)" 

     config LUCI_LANG_da
	   tristate "Danish (da)" 

     config LUCI_LANG_de
	   tristate "German (de)" 

     config LUCI_LANG_el
