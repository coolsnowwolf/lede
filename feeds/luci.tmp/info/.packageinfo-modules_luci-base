Config:
   config LUCI_SRCDIET
	bool "Minify Lua sources"
	default n

   menu "Translations"

     config LUCI_LANG_hu
	   tristate "Hungarian (hu)"
	   default n 

     config LUCI_LANG_pt
	   tristate "Portuguese (pt)"
	   default n 

