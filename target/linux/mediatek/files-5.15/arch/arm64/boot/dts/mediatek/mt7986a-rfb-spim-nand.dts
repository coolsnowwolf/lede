/dts-v1/;
/plugin/;

/ {
        compatible = "mediatek,mt7986a-spim-snand-rfb";

        fragment@0 {
		target-path = "/soc/spi@1100a000";
                __overlay__ {
			status = "okay";
			spi_nand: spi_nand@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "spi-nand";
				reg = <1>;
				spi-max-frequency = <10000000>;
				spi-tx-buswidth = <4>;
				spi-rx-buswidth = <4>;

				partitions {
					compatible = "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;
					partition@0 {
						label = "BL2";
						reg = <0x00000 0x0100000>;
						read-only;
					};
					partition@100000 {
						label = "u-boot-env";
						reg = <0x0100000 0x0080000>;
					};
					factory: partition@180000 {
						label = "Factory";
						reg = <0x180000 0x0200000>;
					};
					partition@380000 {
						label = "FIP";
						reg = <0x380000 0x0200000>;
					};
					partition@580000 {
						label = "ubi";
						reg = <0x580000 0x4000000>;
					};
				};
			};
		};
        };
};
