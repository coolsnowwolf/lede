. /lib/functions/leds.sh
. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case $board in
linksys,e8450|\
linksys,e8450-ubi|\
netgear,wax206)
	ucidef_set_led_netdev "wan" "WAN" "inet:blue" "wan"
	;;
xiaomi,redmi-router-ax6s)
	ucidef_set_led_netdev "wan" "WAN" "blue:net" "wan"
	;;
esac

board_config_flush

exit 0
