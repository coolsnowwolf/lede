		;;
	dap-2695-a1)
		ucidef_add_switch "switch0" "0@eth0" "2:lan" "3:wan" "6@eth1"
		;;
	rb-931-2nd)
		ucidef_add_switch "switch0" \
			"0@eth0" "1:lan:3" "2:lan:2" "3:wan:1"
	;;
