		ucidef_add_switch "switch0" \
			"0@eth0" "2:lan" "3:lan"
		;;
	dw33d|\
	f9k1115v2)
		ucidef_set_interfaces_lan_wan "eth1.1" "eth0.2"
		ucidef_add_switch "switch0" \
			"2:lan" "3:lan" "4:lan" "5:lan" "6@eth1" "0@eth0" "1:wan"
