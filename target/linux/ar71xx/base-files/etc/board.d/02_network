				"0@eth0" "2:lan" "3:lan" "4:lan" "5:lan" "1:wan" "6:sfp"
		;;
		*)
			ucidef_add_switch "switch0" \
				"0@eth0" "2:lan" "3:lan" "4:lan" "5:lan" "1:wan"
		;;
		esac

		ucidef_add_switch "switch1" \
			"0@eth1" "1:lan" "2:lan" "3:lan" "4:lan" "5:lan"
		;;
	dir-615-i1|\
	omy-g1|\
	r6100|\
	smart-300|\
	tl-wdr6500-v2|\
	tl-wdr6500-v6|\
