#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath11k/IPQ6018/hw1.0/cal-ahb-c000000.wifi.bin")
	case "$board" in
	glinet,gl-ax1800|\
	glinet,gl-axt1800|\
	linksys,mr7350|\
	qihoo,360v6)
		caldata_extract "0:art" 0x1000 0x10000
		;;
	redmi,ax5-jdcloud)
		caldata_extract_mmc "0:ART" 0x1000 0x10000
		;;
	esac
	;;
*)
	exit 1
	;;
esac
