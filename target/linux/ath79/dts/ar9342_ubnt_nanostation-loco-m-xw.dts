/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9342_ubnt_xw.dtsi"

/ {
	compatible = "ubnt,nanostation-loco-m-xw", "ubnt,xw", "qca,ar9342";
	model = "Ubiquiti Nanostation Loco M (XW)";
};

&mdio0 {
	status = "okay";

	phy-mask = <0x1>;

	phy1: ethernet-phy@1 {
		reg = <1>;
		phy-mode = "mii";
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&phy1>;
};
