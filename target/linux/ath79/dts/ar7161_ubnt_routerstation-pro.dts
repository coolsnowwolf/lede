/dts-v1/;

#include "ar7161_ubnt_routerstation.dtsi"

/ {
	compatible = "ubnt,routerstation-pro", "qca,ar7161";
	model = "Ubiquiti RouterStation Pro";
};

&mdio0 {
	status = "okay";

	phy0: ethernet-phy@0 {
		reg = <0>;
	};

	phy4: ethernet-phy@4 {
		reg = <4>;
	};
};

&eth0 {
	phy-mode = "rgmii";
	phy-handle = <&phy4>;
};

&eth1 {
	phy-mode = "rgmii";
	phy-handle = <&phy0>;
};
