&leds {
	usb_green {
		label = "green:usb";
		gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		trigger-sources = <&hub_port>;
		linux,default-trigger = "usbport";
	};
};

&ath9k {
	usb_power {
		gpio-hog;
		line-name = "netgear:power:usb";
		gpios = <0 GPIO_ACTIVE_HIGH>;
		output-high;
	};
};

&usb_phy {
	status = "okay";
};

&usb {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};
