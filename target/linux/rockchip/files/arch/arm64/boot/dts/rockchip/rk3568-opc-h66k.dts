/dts-v1/;

#include "rk3568-hinlink-opc.dtsi"

/ {
	model = "HINLINK OPC-H66K Board";
	compatible = "hinlink,opc-h66k", "rockchip,rk3568";

	aliases {
		ethernet0 = &rtl8125_1;
		ethernet1 = &rtl8125_2;
	};
};

&vcc3v3_pcie {
	gpio = <&gpio0 RK_PC4 GPIO_ACTIVE_HIGH>;
};
