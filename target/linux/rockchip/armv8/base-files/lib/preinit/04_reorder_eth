. /lib/functions.sh

preinit_reorder_eth() {
        case $(board_name) in
        firefly,rk3568-roc-pc)
                ip link set eth0 name ethtmp
                ip link set eth1 name eth0
                ip link set ethtmp name eth1
                ;;
        esac
}

boot_hook_add preinit_main preinit_reorder_eth
