. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
avnet,zynq-zed | \
digilent,zynq-zybo | \
digilent,zynq-zybo-z7 | \
xlnx,zynq-zc702)
	ucidef_set_interface_lan 'eth0'
	;;
*)
	echo "Unsupported hardware. Network interfaces not intialized"
	;;
esac

board_config_flush

exit 0
