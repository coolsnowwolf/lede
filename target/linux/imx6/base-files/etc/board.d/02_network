#!/bin/sh
#
# Copyright (C) 2013-2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case "$board" in
cubox-i |\
gw51xx |\
gw52xx |\
gw5904 |\
gw5907 |\
gw5910 |\
gw5912 |\
gw5913)
	ucidef_set_interface_lan 'eth0'
	;;
gw53xx |\
gw54xx |\
gw552x)
	ucidef_set_interfaces_lan_wan 'eth0' 'eth1'
	;;
wandboard)
	ucidef_set_interface_wan 'eth0'
	;;
esac

board_config_flush

exit 0
