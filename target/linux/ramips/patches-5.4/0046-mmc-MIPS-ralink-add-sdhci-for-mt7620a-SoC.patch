 drivers/mmc/host/Kconfig             |    2 +
 drivers/mmc/host/Makefile            |    1 +
 drivers/mmc/host/mtk-mmc/Kconfig     |   16 +
 drivers/mmc/host/mtk-mmc/Makefile    |   42 +
 drivers/mmc/host/mtk-mmc/board.h     |  137 ++
 drivers/mmc/host/mtk-mmc/dbg.c       |  347 ++++
