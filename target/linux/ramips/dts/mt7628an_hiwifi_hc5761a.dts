#include "mt7628an_hiwifi_hc5x61a.dtsi"

/ {
	compatible = "hiwifi,hc5761a", "mediatek,mt7628an-soc";
	model = "HiWiFi HC5761A";

	leds {
		compatible = "gpio-leds";

		led_system: system {
			label = "blue:system";
			gpios = <&gpio 37 GPIO_ACTIVE_HIGH>;
		};

		internet {
			label = "blue:internet";
			gpios = <&gpio 1 GPIO_ACTIVE_HIGH>;
		};

		wlan2g {
			label = "blue:wlan2g";
			gpios = <&gpio 44 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy0tpt";
		};

		wlan5g {
			label = "blue:wlan5g";
			gpios = <&gpio 40 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy1tpt";
		};
	};

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;

		usb_power {
			gpio-export,name = "usb_power";
			gpio-export,output = <0>;
			gpios = <&gpio 41 GPIO_ACTIVE_LOW>;
		};
	};
};

&pinctrl {
	sdxc_router_pins: sdxc_router_mode {
		gpio {
			groups = "i2s", "sdmode", "uart1", "gpio";
			function = "gpio";
		};

		esdmode {
			groups = "esdmode";
			function = "sdxc";
		};
	};

	state_default: pinctrl0 {
		gpio {
			groups = "i2c", "refclk", "wdt", "p2led_an", "p3led_an", "wled_an";
			function = "gpio";
		};
	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	wifi@0,0 {
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&sdhci {
	status = "okay";
	pinctrl-0 = <&sdxc_router_pins>;
};
