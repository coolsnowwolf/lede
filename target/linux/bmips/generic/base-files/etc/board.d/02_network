. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
comtrend,ar-5315u |\
comtrend,ar-5387un |\
comtrend,vr-3025u)
	ucidef_set_bridge_device switch
	ucidef_set_interface_lan "lan1 lan2 lan3 lan4"
	;;
esac

board_config_flush

exit 0
