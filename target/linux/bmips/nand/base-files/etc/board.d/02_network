. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
comtrend,vr-3032u)
	ucidef_set_bridge_device switch
	ucidef_set_interface_lan "lan1 lan2 lan3 lan4"
	;;
netgear,dgnd3700-v2)
	ucidef_set_bridge_device switch
	ucidef_set_interface_lan "extsw"
	;;
esac

board_config_flush

exit 0
