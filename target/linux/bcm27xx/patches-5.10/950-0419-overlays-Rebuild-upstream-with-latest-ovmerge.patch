 			status = "okay";
 		};
 	};
-	fragment@9 {
+	fragment@8 {
 		target = <&vc4>;
 		__overlay__ {
 			status = "okay";
 		};
 	};
-	fragment@10 {
+	fragment@9 {
 		target = <&clocks>;
 		__overlay__ {
 			claim-clocks = <BCM2835_PLLD_DSI0 BCM2835_PLLD_DSI1 BCM2835_PLLH_AUX BCM2835_PLLH_PIX>;
 		};
 	};
-	fragment@11 {
+	fragment@10 {
 		target = <&vec>;
 		__overlay__ {
 			status = "okay";
 		};
 	};
-	fragment@12 {
+	fragment@11 {
 		target = <&txp>;
 		__overlay__ {
 			status = "okay";
 		};
 	};
-	fragment@13 {
-		target = <&hdmi>;
-		__dormant__ {
-			dmas;
-		};
-	};
-	fragment@14 {
+	fragment@12 {
 		target = <&audio>;
 		__overlay__ {
 			brcm,disable-hdmi;
 		};
 	};
-	fragment@15 {
+	fragment@13 {
 		target = <&usb>;
 		#address-cells = <1>;
 		#size-cells = <1>;
