+					#undef cam1_clk
+
 					#define cam_node imx219_3
 					#define cam_endpoint imx219_3_ep
 					#define cam1_clk clk_24mhz
@@ -640,6 +680,12 @@
 	};
 
 	__overrides__ {
+		cam0-arducam-64mp = <&mux_in0>, "remote-endpoint:0=",<&arducam_64mp_0_ep>,
