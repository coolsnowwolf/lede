 					#define cam1_clk clk_24mhz
@@ -334,6 +354,16 @@
 					#address-cells = <1>;
 					#size-cells = <0>;
 
+					#define cam_node arducam_64mp_2
+					#define cam_endpoint arducam_64mp_2_ep
+					#define vcm_node arducam_64mp_2_vcm
