+static int
+safe_memcmp(const void *s1, const void *s2, size_t n)
+{
+    const unsigned char *_s1 = (const unsigned char *)s1;
+    const unsigned char *_s2 = (const unsigned char *)s2;
+    int ret                  = 0;
+    size_t i;
+    for (i = 0; i < n; i++)
+        ret |= _s1[i] ^ _s2[i];
+    return !!ret;
+}
