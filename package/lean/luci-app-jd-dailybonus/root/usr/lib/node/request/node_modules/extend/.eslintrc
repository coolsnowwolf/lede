{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"complexity": [2, 20],
		"eqeqeq": [2, "allow-null"],
		"func-name-matching": [1],
		"max-depth": [1, 4],
		"max-statements": [2, 26],
		"no-extra-parens": [1],
		"no-magic-numbers": [0],
		"no-restricted-syntax": [2, "BreakStatement", "ContinueStatement", "DebuggerStatement", "LabeledStatement", "WithStatement"],
		"sort-keys": [0],
	}
}
