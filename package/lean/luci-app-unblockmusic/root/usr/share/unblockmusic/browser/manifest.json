{
	"name": "UnblockNeteaseMusic",
	"description": "For test (es6 only)",
	"version": "0.1",
	"background": {
		"page": "background.html"
	},
	"content_scripts": [{
		"js": ["script.js"],
		"matches": ["*://music.163.com/*"],
		"all_frames": true
	}],
	"web_accessible_resources": ["inject.js"],
	"externally_connectable": {
		"matches": ["*://music.163.com/*"]
	},
	"manifest_version": 2,
	"permissions": ["*://*/*", "webRequest", "webRequestBlocking"]
}