+static uint8_t PowervuCreateDataCw(uint8_t *seed, uint8_t lenSeed, uint8_t *baseCw, uint8_t val, uint8_t *seedEcmCw, uint8_t *data)
+{
+	int i;
+	
+	for(i = 0; i < lenSeed; i++)
+	{
+		data[i] = seed[i];
+	}
+	
+	for(i = 0; i < 7; i++)
+	{
+		data[lenSeed + i] = baseCw[i];
+	}
+	
+	data[lenSeed + 7] = val;
+	
+	for(i = 0; i < 16; i++)
+	{
+		data[lenSeed + 7 + 1 + i] = seedEcmCw[i];
+	}
+	
+	return lenSeed + 7 + 1 + 0x10;
