if FIRST_IF_MT7603E || SECOND_IF_MT7603E
config MT7603E_RLT_WIFI
	tristate "RLT 7603 WIFI Driver"
	select MT7603E_WIFI_BASIC_FUNC if MT7603E_RLT_WIFI

config MT7603E_MT_WIFI_PATH
        string
        depends on MT7603E_RLT_WIFI
        default "rlt_wifi"

if MT7603E_RLT_WIFI
