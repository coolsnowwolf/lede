        string
        depends on ! FIRST_IF_NONE
        default "prom" if MT7603E_FIRST_IF_EEPROM_PROM
        default "efuse" if MT7603E_FIRST_IF_EEPROM_EFUSE
        default "flash" if MT7603E_FIRST_IF_EEPROM_FLASH
endif
# endif FIRST_IF_MT7603E

