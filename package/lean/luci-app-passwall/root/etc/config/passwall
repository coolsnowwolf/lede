	option tcp_proxy_mode 'chnroute'
	option udp_proxy_mode 'chnroute'
	option localhost_tcp_proxy_mode 'gfwlist'
	option localhost_udp_proxy_mode 'gfwlist'
	option socks_server '0.0.0.0:1080'

