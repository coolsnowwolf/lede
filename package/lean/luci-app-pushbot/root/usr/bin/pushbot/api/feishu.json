{
    "_api": "这是飞书推送 post 模板信息 api 文件",
    "_api": "【飞书推送】",
    
    "url": "https://open.feishu.cn/open-apis/bot/v2/hook/${fs_webhook}",
    "data": "@${tempjsonpath}",
    "content_type": "Content-Type: application/json",
    "str_title_start": "**",
    "str_title_end": "**",
    "str_linefeed": "\\n",
    "str_splitline": "\\n\\n",
    "str_space": " ",
    "str_tab": " ",
    "table_tab": "",
    "font_green": "",
    "font_green": "",
    "font_red": "",
    "font_blue": "",
    "font_purple": "",
    "font_end": "",
    "font_end2": "",
    "percentsym": "",
    "boldstar": "**",
    "type":
	{
		"msg_type": "\"interactive\"",
		"card": {
			"config": {
				"wide_screen_mode": "true"
			},
			"header": {
				"template": "\"purple\"",
				"title": {
					"content": "\"${1}\"",
					"tag": "\"plain_text\""
				}
			},
			"elements": [
			{
				"tag": "\"div\"",
				"text": {
					"content": "\"${nowtime}${str_linefeed}${2}\"",
					"tag": "\"lark_md\""
				}
			},
			{
				"tag": "\"hr\""
			},
			{
				"elements": [
				{
					"content": "\"来自${device_name}\"",
					"tag": "\"lark_md\""
				}
				],
			"tag": "\"note\""
			}
			]
		}
	}
}
