msgid "ADBYBY Plus +"
msgstr "广告屏蔽大师 Plus+"

msgid "<strong><font color=\"green\">Adbyby Plus + is Running</font></strong>"
msgstr "<strong><font color=\"green\">广告屏蔽大师 Plus + 正在运行</font></strong>"

msgid "<strong><font color=\"red\">Adbyby Plus + is Not Running</font></strong>"
