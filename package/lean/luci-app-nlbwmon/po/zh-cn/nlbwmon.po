msgid "%d IPv4-only hosts"
msgstr "%d 个主机仅支持 IPv4"

msgid "%d IPv6-only hosts"
msgstr "%d 个主机仅支持 IPv6"

msgid "%d dual-stack hosts"
msgstr "%d 个双协议栈主机"

msgid "%s and %s"
msgstr "%s 和 %s"

