-    FDKmemcpy(sfmSbr[est], sfmSbr[est + noEstPerFrame],
-              sizeof(FIXP_DBL) * MAX_FREQ_COEFFS);
-  }
-  C_ALLOC_SCRATCH_END(_scratch, FIXP_DBL,
-                      3 * MAX_NO_OF_ESTIMATES / 2 * MAX_FREQ_COEFFS)
-}
