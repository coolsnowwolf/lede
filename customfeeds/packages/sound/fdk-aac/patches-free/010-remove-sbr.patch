-                   (INT)h_sbr->sbrExtractEnvelope.envelopeCompensation[j]));
-        }
-        sfb_nrgLeft[m] = fixMax(0, sfb_nrgLeft[m]);
-        m++;
-      }
-    } /* useParametricCoding */
-
-  } /* env loop */
