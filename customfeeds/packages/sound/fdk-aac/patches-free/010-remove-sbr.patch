-      break;
-    case PS_BANDS_MID:
-      mode = PS_RES_MID;
-      break;
-    default:
-      mode = 0;
-  }
-  if (rotType == PS_ICC_ROT_B) {
-    mode += 3;
-  }
-
-  return mode;
-}
