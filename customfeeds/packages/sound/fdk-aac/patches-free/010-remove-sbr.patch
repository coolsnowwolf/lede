-  /* Clear reset and flush flag because everything seems to be done
-   * successfully. */
-  self->flags &= ~SBRDEC_FORCE_RESET;
-  self->flags &= ~SBRDEC_FLUSH;
-
-bail:
-
-  return errorStatus;
-}
