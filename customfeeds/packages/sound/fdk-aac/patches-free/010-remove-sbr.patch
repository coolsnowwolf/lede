-    delta_exp = target_exp - source_exp;
-
-    if (delta_exp < 0) { /* x-over-area is dominant */
-      startBand = target_lsb;
-      stopBand = target_usb;
-      delta_exp = -delta_exp;
-
-      if (new_lsb > old_lsb) {
