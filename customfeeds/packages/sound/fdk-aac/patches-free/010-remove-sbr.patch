-void FDKsbrEnc_Shellsort_fract(FIXP_DBL *in, INT n) {
-  FIXP_DBL v;
-  INT i, j;
-  INT inc = 1;
-
-  do
-    inc = 3 * inc + 1;
-  while (inc <= n);
-
-  do {
-    inc = inc / 3;
-    for (i = inc + 1; i <= n; i++) {
-      v = in[i - 1];
-      j = i;
-      while (in[j - inc - 1] > v) {
-        in[j - 1] = in[j - inc - 1];
-        j -= inc;
-        if (j <= inc) break;
-      }
-      in[j - 1] = v;
-    }
-  } while (inc > 1);
-}
-
-/* Sorting routine */
-void FDKsbrEnc_Shellsort_int(INT *in, INT n) {
-  INT i, j, v;
-  INT inc = 1;
-
-  do
-    inc = 3 * inc + 1;
-  while (inc <= n);
-
-  do {
-    inc = inc / 3;
-    for (i = inc + 1; i <= n; i++) {
-      v = in[i - 1];
-      j = i;
-      while (in[j - inc - 1] > v) {
-        in[j - 1] = in[j - inc - 1];
-        j -= inc;
-        if (j <= inc) break;
-      }
-      in[j - 1] = v;
-    }
-  } while (inc > 1);
-}
