-    }
-
-    if (pBsData->bEnableIcc) {
-      pBsData->freqResIcc = h_ps_d->specificTo.mpeg.prevFreqResIcc;
-      for (gr = 0; gr < NO_HI_RES_ICC_BINS; gr++) {
-        pBsData->aaIccIndex[pBsData->noEnv - 1][gr] =
