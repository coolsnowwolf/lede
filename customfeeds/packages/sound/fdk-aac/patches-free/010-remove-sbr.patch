-      } else {
-        /* The highband has to be rescaled */
-        hSbrDec->qmfDomainInCh->scaling.ov_hb_scale = EXP2SCALE(source_exp);
-      }
-    }
-
-    FDK_ASSERT(startBand <= stopBand);
