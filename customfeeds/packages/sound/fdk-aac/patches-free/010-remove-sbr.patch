-        /* ld64 to integer conversion */
-        nrgRight = (FIXP_DBL)(LONG)(nrgRight) >>
-                   (DFRACT_BITS - 1 - LD_DATA_SHIFT - 1 - oneBitLess);
-        nrgRight = (nrgRight + (FIXP_DBL)1) >> 1; /* rounding */
