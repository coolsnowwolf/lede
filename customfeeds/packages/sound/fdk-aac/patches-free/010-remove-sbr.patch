-/* ***************************** crcAdvance **********************************/
-/**
- * @fn
- * @brief    updates crc data register
- * @return   none
- *
- * This function updates the crc register
- *
- */
-static void crcAdvance(USHORT crcPoly, USHORT crcMask, USHORT *crc,
-                       ULONG bValue, INT bBits) {
-  INT i;
-  USHORT flag;
-
-  for (i = bBits - 1; i >= 0; i--) {
-    flag = ((*crc) & crcMask) ? (1) : (0);
-    flag ^= (bValue & (1 << i)) ? (1) : (0);
-
-    (*crc) <<= 1;
-    if (flag) (*crc) ^= crcPoly;
-  }
-}
