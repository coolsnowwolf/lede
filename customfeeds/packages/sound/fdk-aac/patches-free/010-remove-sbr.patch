-            fMin(DFRACT_BITS - 1, new_summand_sf - total_power_high_sf);
-        total_power_high_sf = new_summand_sf;
-      } else if (new_summand_sf < total_power_high_sf) {
-        new_summand >>=
-            fMin(DFRACT_BITS - 1, total_power_high_sf - new_summand_sf);
-      }
