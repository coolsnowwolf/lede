-      FDKsbrEnc_AddVecLeft(v_bord, length_v_bord, v_bordFollow,
-                           *length_v_bordFollow);
-      FDKsbrEnc_AddVecLeft(v_freq, length_v_freq, v_freqFollow,
-                           *length_v_freqFollow);
-    }
-  }
