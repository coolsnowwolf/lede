-INT sbrEncoder_GetInputDataDelay(HANDLE_SBR_ENCODER hSbrEncoder) {
-  INT delay = -1;
-
-  if (hSbrEncoder) {
-    delay = hSbrEncoder->inputDataDelay;
-  }
-  return delay;
-}
-
-INT sbrEncoder_GetSbrDecDelay(HANDLE_SBR_ENCODER hSbrEncoder) {
-  INT delay = -1;
-
-  if (hSbrEncoder) {
-    delay = hSbrEncoder->sbrDecDelay;
-  }
-  return delay;
-}
