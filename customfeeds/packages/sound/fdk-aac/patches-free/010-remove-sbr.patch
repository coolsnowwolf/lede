-****************************************************************************/
-int CreatePsDec(HANDLE_PS_DEC *h_PS_DEC, /*!< pointer to the module state */
-                int aacSamplesPerFrame) {
-  SBR_ERROR errorInfo = SBRDEC_OK;
-  HANDLE_PS_DEC h_ps_d;
-  int i;
-
-  if (*h_PS_DEC == NULL) {
