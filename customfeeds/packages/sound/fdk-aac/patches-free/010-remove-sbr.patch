-  /* PS data from bitstream (if avail) was decoded now */
-  h_ps_d->bPsDataAvail[h_ps_d->processSlot] = ppt_none;
-
-  /* handling of env borders for FIX & VAR */
-  if (pBsData->bFrameClass == 0) {
-    /* FIX_BORDERS NoEnv=0,1,2,4 */
